{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,  
      "baseUrl": ".",
      "paths": {
        "@/components/*": ["./src/components/*"],
        "@/utils/*": ["./src/utils/*"],
        "@/hooks/*": ["./src/utils/*"],
        "@/network/*": ["./src/network/*"],
        "@/store/*": ["./src/store/*"],
      } 
  }
}
